{
  "name": "nitro-example-caster-api",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "biome-check": "biome lint --error-on-warnings --diagnostic-level=warn --config-path=./biome.jsonc .",
    "biome-fix": "biome lint --write --config-path=./biome.jsonc .",
    "biome-format-select": "biome format --write --config-path=./biome.jsonc",
    "biome-format": "pnpm biome-format-select ."
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0-beta.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "dotenv-cli": "^8.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "tsx": "^4.19.3",
    "turbo": "^2.4.2",
    "type-fest": "^4.35.0",
    "typescript": "^5.7.3"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/engines",
      "prisma"
    ]
  }
}
